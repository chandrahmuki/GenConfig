9046ba62ffc6fa3c22ed8a417c00750b|file:///home/david/.config/polybar/launch.sh|1|1|v|sh
	#!/usr/bin/env sh
	
	# Terminate already running bar instances
	killall -q polybar
	
	# Wait until the processes have been shut down
	while pgrep -u $UID -x polybar >/dev/null; do sleep 1; done
	
	# Launch bar1 and bar2
	polybar example &
	#polybar bar2 &
	
	echo "Bars launched..."
